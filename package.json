{
  "name": "heatherallard-ca",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jordond.github.io/heatherallard-ca",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:css": "stylelint src/**/*.css --fix",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.9.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "stylelint": "^8.3.1",
    "stylelint-config-prettier": "^2.0.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
