{
  "name": "heatherallard-ca",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jordond.github.io/heatherallard-ca",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:css": "stylelint src/**/*.css --fix",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build && cpx circle.yml build/",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "react-typist": "^2.0.4",
    "shortid": "^2.2.8",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "prop-types": "^15.6.0",
    "stylelint": "^8.4.0",
    "stylelint-config-prettier": "^2.0.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
